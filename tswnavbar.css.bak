#tswcsstabs ul {
	margin: 0 ;
	padding: 0 ;
	list-style: none ;
	display: inline ;
}
#tswcsstabs ul li {
	margin: 0 ;
	padding: 0 ;
	display: inline ;
	text-align: center ;
	float: left ;
	font-family: Arial, Helvetica, sans-serif ;
}
#tswcsstabs li a {
	color: #000000 ;
	background-color: #ffb200 ;
	border: 1px outset #b37d00 ;
	padding: 8px ;
	text-decoration: none ;
	display: inline ;
}
#tswcsstabs li a:hover {
	color: #ffffff ;
	background-color: #b37d00 ;
}
